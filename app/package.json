{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack -w -d --display-error-details",
    "test": "cross-env NODE_ENV=test mochapack --interactive false --require test/setup.js \"test/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "bootstrap-vue": "2.0.1",
    "cross-env": "^5.0.5",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "expect": "24.8.0",
    "jest-mock": "24.8.0",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.1",
    "mochapack": "1.1.5",
    "uglifyjs-webpack-plugin": "^1.1.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
